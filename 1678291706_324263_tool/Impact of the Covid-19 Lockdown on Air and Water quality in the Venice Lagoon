   "source": [
    "### Plot NO2 from air-quality forecasts over the Venice Lagoon"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "9c807155-baa0-476b-b341-089783c5a452",
   "metadata": {},
   "outputs": [],
   "source": [
    "def plot_stats(variable, lat, lon, df): \n",
    "    dt = df.sel(latitude=lat, longitude=lon, method=\"nearest\").to_dataframe()\n",
    "    dt.reset_index(inplace=True)\n",
    "    dt['datetime'] = pd.DatetimeIndex(dt['time']).year\n",
    "    dt[\"year\"] = pd.DatetimeIndex(dt['time']).year\n",
    "    dt = dt.groupby(\"datetime\").mean()\n",
    "    dt['name'] = 'Venice Lagoon'\n",
    "    ax = sns.catplot(x=\"name\", y=variable, hue=\"year\", kind=\"bar\", data=dt, height=7, aspect=2.5)\n",
    "    \n",
    "    ax.fig.suptitle('Total ' + variable + ' averaged (March-June) over the Venice Lagoon')\n",
    "    return dt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "7dee0133-a640-4cd7-ba0c-8b3d809b44ba",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
